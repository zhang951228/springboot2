<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.erayt.dbconnector.repository.UserMapper">
    <select id="queryById" parameterType="java.lang.String" resultType="com.erayt.dbconnector.domain.User">
        SELECT *
        FROM user
        WHERE id = #{id}
    </select>
</mapper>